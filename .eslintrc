{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
      "require": true
    },
    "rules": {
        "react/display-name": "off",
        "react/no-find-dom-node": "off",
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off"
    }
}
